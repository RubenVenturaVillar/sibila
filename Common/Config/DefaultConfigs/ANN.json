{
    "model": "ANN",
    "type_ml": "classification",
    "n_job": 1,
    "params": {
        "draw_model": true,
        "random_state": 500,
        "cv_splits": 10,
        "cv_metrics": [
            "tensorflow.keras.metrics.MeanSquaredLogarithmicError",
            "tensorflow.keras.metrics.RootMeanSquaredError"
        ],
        "batch_size": 1024
    },
    "params_grid": {
        "objective": ["accuracy"],
        "max_layers": 3,
        "min_units": 4,
        "max_units": 512,
        "step_units": 4,
        "output_units": 2,
        "activation": ["relu","elu","tanh","sigmoid","softmax","linear","exponential"],
        "dropout": true,
        "dropout_rate": 0.25,
        "min_lr": 1e-4,
        "max_lr": 1e-2,
        "sampling_lr": "log",
        "metrics": ["accuracy"],
        "loss_function": "sparse_categorical_crossentropy",
        "epochs": 100,
        "executions_per_trial": 5,
        "early_stopping_patience": 3
    }
}
